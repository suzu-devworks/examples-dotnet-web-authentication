{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Identity(web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Examples.Web.Authentication.Identity/bin/Debug/net6.0/Examples.Web.Authentication.Identity.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Examples.Web.Authentication.Identity",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "Basic(web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Examples.Web.Authentication.Basic/bin/Debug/net6.0/Examples.Web.Authentication.Basic.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Examples.Web.Authentication.Basic",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
